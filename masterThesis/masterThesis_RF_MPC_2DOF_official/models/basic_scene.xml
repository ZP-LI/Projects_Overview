<mujoco>
    <compiler angle="degree"/>

    <asset>
        <texture type="skybox" builtin="gradient" rgb1="1 1 1" rgb2=".6 .8 1" width="256" height="256"/>
        <texture name="texplane" type="2d" builtin="checker" rgb1=".9 .9 .9" rgb2="1 1 1" width="512" height="512" mark="cross" markrgb=".1 .1 .1"/>  

        <material name="matplane" reflectance="0.3" texture="texplane" texrepeat="2 2" texuniform="true"/>
    </asset>

    <option timestep="0.001" o_solimp="0.9 0.95 0.001 0.5 2" o_solref="0.02 1" cone="elliptic" 
        impratio="3.0" noslip_iterations="5" noslip_tolerance="1e-8">
        <flag override="enable"/>
    </option>

    <default>
        <geom type="box" mass="0.001" rgba=".95 .95 .95 1"/>
    </default>
</mujoco>